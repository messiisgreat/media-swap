{
  "name": "media-swap",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "package:install": "pnpm install",
    "dev": "next dev",
    "build": "prisma generate && ts-node --project tsconfig.build.json -r tsconfig-paths/register src/constants/constantsSeed.ts && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "db:dev": "docker-compose up -d",
    "db:seed": "prisma db seed",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:show": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "depcheck": "pnpx depcheck",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": [
      "pnpm prettier . --write"
    ]
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.9",
    "@prisma/client": "^5.6.0",
    "autoprefixer": "10.4.16",
    "daisyui": "^4.4.14",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "embla-carousel-react": "7.1.0",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "heic2any": "^0.0.4",
    "jotai": "^2.6.0",
    "mongodb": "^6.3.0",
    "next": "14.0.3",
    "next-auth": "4.24.5",
    "next-compose-middleware": "^1.0.0",
    "nodemailer": "^6.9.7",
    "postcss": "8.4.32",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-google-recaptcha-v3": "^1.10.1",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.12.0",
    "react-remove-scroll": "^2.5.7",
    "react-tag-input": "^6.8.1",
    "server-only": "^0.0.1",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "3.3.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^10.6.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@types/react-google-recaptcha": "^2.1.8",
    "@types/react-tag-input": "^6.6.5",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.6.0",
    "prisma-erd-generator": "^1.11.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
